{
	"name": "@thatskyapplication/caelus",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "tsc",
		"test": "tsc --noEmit",
		"format": "prettier --write . && eslint --fix .",
		"lint": "prettier --check . && eslint .",
		"dev": "tsx --require dotenv/config source/index.ts | pino-pretty",
		"start": "node --enable-source-maps --require dotenv/config distribution/index.js",
		"update": "pnpm update --interactive --latest && pre-commit autoupdate",
		"knip": "knip"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.600.0",
		"@logtail/pino": "^0.4.22",
		"discord.js": "14.15.3",
		"dotenv": "^16.4.5",
		"hasha": "^6.0.0",
		"i18next": "^23.11.5",
		"knex": "^3.1.0",
		"luxon": "^3.4.4",
		"openai": "^4.52.0",
		"p-queue": "^8.0.1",
		"pg": "^8.12.0",
		"pino": "^9.2.0",
		"sharp": "^0.33.4",
		"ts-mixer": "^6.0.4"
	},
	"devDependencies": {
		"@types/luxon": "^3.4.2",
		"eslint": "^9.5.0",
		"eslint-config-neon": "^0.1.62",
		"knip": "^5.22.2",
		"pino-pretty": "^11.2.1",
		"prettier": "^3.3.2",
		"tsx": "^4.15.7",
		"typescript": "5.4.5"
	},
	"packageManager": "pnpm@9.1.4",
	"pnpm": {
		"patchedDependencies": {
			"discord.js@14.15.3": "patches/discord.js@14.15.3.patch"
		}
	}
}
