{
	"name": "@thatskyapplication/caelus",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "tsc",
		"test": "tsc --noEmit",
		"format": "prettier --write . && eslint --fix .",
		"lint": "prettier --check . && eslint .",
		"dev": "tsx --require dotenv/config source/index.ts",
		"start": "node --enable-source-maps --require dotenv/config distribution/index.js",
		"update": "pnpm update --interactive --latest"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.409.0",
		"@sapphire/async-queue": "^1.5.0",
		"dayjs": "^1.11.9",
		"discord.js": "14.13.0",
		"dotenv": "^16.3.1",
		"hasha": "^5.2.2",
		"knex": "^2.5.1",
		"openai": "^4.6.0",
		"pg": "^8.11.3",
		"sharp": "^0.32.5",
		"ts-mixer": "^6.0.3",
		"undici": "^5.24.0"
	},
	"devDependencies": {
		"eslint": "^8.49.0",
		"eslint-config-neon": "^0.1.56",
		"prettier": "^3.0.3",
		"tsx": "^3.12.8",
		"typescript": "^5.2.2"
	},
	"packageManager": "pnpm@8.7.1"
}
