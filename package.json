{
  "name": "caelus",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm install && npm run-script clean && tsc",
    "clean": "rimraf sourceJS/",
    "prepare": "husky install",
    "start": "npm run-script build && BRANCH=$(git rev-parse --abbrev-ref HEAD) COMMIT_HASH=$(git rev-parse HEAD) ENVIRONMENT=Production node --require dotenv/config sourceJS/Caelus.js",
    "test": "eslint . && tsc"
  },
  "dependencies": {
    "date-fns-tz": "^1.3.6",
    "discord.js": "^14.0.3",
    "dotenv": "^16.0.1",
    "mariadb": "^3.0.0",
    "node-fetch": "^3.2.9"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "eslint": "^8.20.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4"
  }
}
