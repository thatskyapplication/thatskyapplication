{
	"$schema": "node_modules/knip/schema.json",
	"ignoreBinaries": ["pre-commit"],
	"ignoreDependencies": ["@types/*", "pg"],
	"workspaces": {
		"apps/{notifications,socials}": {
			"entry": "source/sentry.ts"
		},
		"apps/caelus": {
			"entry": "source/sentry.ts",
			"ignoreMembers": [
				"StandardSpirits",
				"Elders",
				"SeasonalSpirits",
				"Events",
				"AboutFeedback",
				"AboutIssue",
				"AboutFeedbackModal",
				"AboutFeedbackModalTitle",
				"AboutFeedbackModalDescription",
				"AboutIssueModal",
				"AboutIssueModalTitle",
				"AboutIssueModalDescription",
				"AboutIssueModalAttachments",
				"AIFrequency",
				"MeSetBio",
				"MeSetBioModal",
				"MeSetBioModalBio",
				"WelcomeChannel",
				"WelcomeMessage",
				"WelcomeMessageDelete",
				"WelcomeHugEdit",
				"WelcomeAccentColour",
				"WelcomeAccentColourDelete",
				"WelcomeMessageModal",
				"WelcomeMessageModalMessage",
				"WelcomeAccentColourModal",
				"WelcomeAccentColourModalAccentColour"
			]
		},
		"apps/website": {
			"entry": ["app/server.ts"]
		}
	}
}
