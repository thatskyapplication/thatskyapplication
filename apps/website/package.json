{
	"name": "@thatskyapplication/website",
	"private": true,
	"type": "commonjs",
	"scripts": {
		"build": "npm run-script css && next build",
		"css": "unocss 'src/**/*.tsx' --out-file=src/styles/unocss.css",
		"dev": "concurrently 'npm run-script dev:contentlayer' 'npm run-script dev:css' 'next dev'",
		"dev:contentlayer": "contentlayer dev",
		"dev:css": "unocss 'src/**/*.tsx' --out-file=src/styles/unocss.css --watch",
		"lint": "next lint",
		"start": "next start"
	},
	"dependencies": {
		"@unocss/reset": "^0.51.12",
		"@vercel/analytics": "^1.0.1",
		"ariakit": "^2.0.0-next.44",
		"contentlayer": "^0.3.2",
		"dayjs": "^1.11.7",
		"next": "13.3.4",
		"next-contentlayer": "^0.3.2",
		"next-themes": "^0.2.1",
		"react": "18.2.0",
		"react-custom-scrollbars-2": "^4.5.0",
		"react-dom": "18.2.0",
		"react-icons": "^4.8.0",
		"rehype-slug": "^5.1.0",
		"remark-gfm": "^3.0.1",
		"typescript": "5.0.4"
	},
	"devDependencies": {
		"@jiralite/eslint-config-neon": "^0.1.40",
		"@types/node": "18.16.3",
		"@types/react": "18.2.0",
		"@types/react-dom": "18.2.1",
		"@unocss/cli": "^0.51.12",
		"concurrently": "^8.0.1",
		"eslint": "^8.39.0",
		"prettier": "^2.8.8",
		"unocss": "^0.51.12"
	}
}
