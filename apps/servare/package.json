		"@sapphire/string-store": "^2.0.0",
		"@sentry/node": "^10.20.0",
		"croner": "^9.1.0",
		"diff": "^7.0.0",
		"knex": "^3.1.0",
		"pg": "^8.16.3",
		"pino": "^9.9.0"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.0",
		"@types/diff": "^7.0.2",
		"@types/node": "^22.18.11",
		"knip": "^5.62.0",
		"pino-pretty": "^13.1.1",
		"tsx": "^4.20.4",
		"typescript": "5.9.2"
	}
